{
  "name": "carlonicora/minimaliser-frontend",
  "description": "Front End Minimaliser for Minimalism",
  "version": "1.0.0",
  "time": "2022-05-20",
  "type": "project",
  "minimum-stability": "beta",
  "authors": [
    {
      "name": "Carlo Nicora",
      "email": "carlo@carlonicora.com",
      "homepage": "http://carlonicora.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-readline": "*",
    "carlonicora/jsonapi": "~3.0",
    "carlonicora/minimalism": "~13.4",
    "carlonicora/minimalism-service-twig": "~2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9"
  },
  "scripts": {
    "post-install-cmd": "bash vendor/bin/minimalism-post-install.sh"
  },
  "autoload": {
    "psr-4": {
      "CarloNicora\\Minimalism\\MinimaliserFrontEnd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarloNicora\\Minimalism\\MinimaliserFrontEnd\\Tests\\": "tests/"
    }
  }
}